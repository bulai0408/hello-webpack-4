{
  "presets": [["@babel/preset-env"], ["@babel/preset-react"]],
  "plugins": [
    ["babel-plugin-styled-components"],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "react-hot-loader/babel"
  ]
}
