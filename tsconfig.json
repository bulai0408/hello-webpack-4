{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@api/*": [
        "src/api/*"
      ],
      "@baseUI/*": [
        "src/baseUI/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@types": [
        "src/types"
      ]
    }
  }
}